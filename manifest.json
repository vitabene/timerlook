{
  "name": "timerlook",
  "description": "a custom look for e.ggtimer",
  "version": "2.0",
  "web_accessible_resources": [
        "look.css"
  ],
  "content_scripts": [
   {
      "matches":    ["http://e.ggtimer.com/*"],
      "css":        ["look.css"],
      "js":         [ "init.js"]
   }
  ],  "permissions": [
      "storage"],
  "browser_action": {
    "default_popup": "./popup.html"
  },
  "manifest_version": 2
}
